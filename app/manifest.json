{
  "manifest_version": 2,
  "name": "dl-annotator",
  "version": "0.1.0",
  "description": "create image annotation for object detection and localization",
  "author": "eisneim<eisneim1@gmail.com>",
  "homepage_url": "http://blog.glexe.com",
  "icons": {
    "16": "img/logo-16.png",
    "48": "img/logo-48.png",
    "128": "img/logo-128.png"
  },
  "browser_action": {
    "default_icon": "img/logo-48.png",
    "default_popup": "popup.html"
  },
  "offline_enabled": true,
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "activeTab", "tabs", "storage", "notifications", "contextMenus",
    "<all_urls>"
  ],
  "web_accessible_resources":[
    "fonts/*"
  ],
  "background": {
    "persistent": true,
    "scripts": ["livereload.js","js/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["css/app.css"],
      "js": [
        "js/app_cs.js"
      ]
    }
  ],
  "commands": {}
}